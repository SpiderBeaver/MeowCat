{
  "name": "meowcat",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-server": "cd ./MeowCat_server && npm run build",
    "build-client": "cd ./MeowCat_client && npm run build",
    "clear_old_build": "rm -rf ./build && mkdir .\\build",
    "copy_server_build": "cp -r ./MeowCat_server/dist ./build/dist && cp ./MeowCat_server/.env ./build",
    "copy_client_build": "cp -r ./MeowCat_client/build ./build/dist/client",
    "copy_dependencies": "cp -r ./MeowCat_server/node_modules ./build/node_modules",
    "build": "npm run clear_old_build && npm run copy_server_build && npm run copy_client_build && npm run copy_dependencies"
  },
